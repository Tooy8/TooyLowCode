{
  "name": "imgpond",
  "version": "0.6.2",
  "private": false,
  "description": "Image uploading one-stop solution.",
  "author": "cloydlau",
  "license": "MIT",
  "keywords": [
    "img",
    "image",
    "pic",
    "picture",
    "upload",
    "uploader",
    "vue",
    "crop",
    "cropper",
    "cropperjs",
    "sortable",
    "element-ui",
    "dropzone",
    "filepond"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/imgpond.umd.js",
      "import": "./dist/imgpond.mjs",
      "browser": "./dist/imgpond.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/imgpond.umd.js",
  "module": "./dist/imgpond.mjs",
  "unpkg": "./dist/imgpond.umd.js",
  "jsdelivr": "./dist/imgpond.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npx case-police --fix && npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-ui": ">=2",
    "pic-viewer": ">=0",
    "vue": "2"
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "cropperjs": "latest",
    "kikimore": "latest",
    "lodash-es": "latest",
    "sortablejs": "latest",
    "sweetalert2": "latest",
    "sweetalert2-preset": "latest",
    "vue-cropperjs": "^4.2.0",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@vitejs/plugin-vue2": "latest",
    "axios": "latest",
    "axios-shortcut": "latest",
    "case-police": "latest",
    "element-ui": "latest",
    "eslint": "latest",
    "husky": "latest",
    "json-editor-vue": "latest",
    "lint-staged": "latest",
    "pic-viewer": "latest",
    "sass": "latest",
    "unocss": "0.43.1",
    "unplugin-auto-import": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vue": "2"
  },
  "lint-staged": {
    "!(demo/**/*)*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}