{
  "name": "kayran",
  "version": "0.4.9",
  "description": "A scattered collection of utilities",
  "author": "cloydlau",
  "private": false,
  "license": "MIT",
  "main": "./lib/kayran.umd.js",
  "module": "./dist/kayran.es.js",
  "exports": {
    ".": {
      "import": "./dist/kayran.es.js",
      "require": "./dist/kayran.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "cov": "jest --ci --coverage",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "jest": "jest --watchAll",
    "pretest": "npm run build",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "change-case": "latest",
    "qs": "6.9.6"
  },
  "peerDependencies": {
    "lodash-es": "4"
  },
  "peerDependenciesMeta": {
    "lodash-es": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/expect-puppeteer": "latest",
    "@types/jest-environment-puppeteer": "latest",
    "@types/puppeteer": "latest",
    "chalk": "^4.1.2",
    "codecov": "latest",
    "enquirer": "latest",
    "execa": "^4.1.0",
    "jest": "latest",
    "jest-dev-server": "latest",
    "jest-html-reporter": "latest",
    "jest-lite": "latest",
    "jest-puppeteer": "latest",
    "lodash-es": "latest",
    "minimist": "latest",
    "puppeteer": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "sass": "latest",
    "semver": "latest",
    "ts-jest": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "latest"
  },
  "files": [
    "dist"
  ]
}
