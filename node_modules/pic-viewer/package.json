    "img previewer",
    "picture viewer",
    "picture gallery",
    "picture previewer",
    "viewerjs",
    "vue-viewerjs",
    "qrcode",
    "swiper",
    "carousel",
    "masonry"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/pic-viewer"
  },
  "main": "dist/pic-viewer.umd.js",
  "module": "./dist/pic-viewer.mjs",
  "files": [
    "dist"
