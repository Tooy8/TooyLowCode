  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "kayran": "latest",
    "lodash-es": "latest",
    "qrcode": "latest",
    "swiper": "latest",
