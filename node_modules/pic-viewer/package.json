  },
  "peerDependencies": {
    "element-ui": "2",
    "vue": "2"
  },
  "peerDependenciesMeta": {
    "element-ui": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cloydlau/scripts": "latest",
    "@vitejs/plugin-vue2": "latest",
    "element-ui": "latest",
    "sass": "latest",
    "typescript": "latest",
    "unocss": "0.43.1",
    "vite": "latest",
    "vue": "2"
  }
