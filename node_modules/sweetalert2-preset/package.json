{
  "name": "sweetalert2-preset",
  "version": "0.0.0",
  "private": false,
  "description": "Provide out-of-the-box presets for [sweetalert2](https://sweetalert2.github.io/).",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/sweetalert2-preset",
  "keywords": [
    "sweetalert",
    "sweetalert2",
    "element-ui",
    "message",
    "message-box",
    "element-plus"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/sweetalert2-preset.umd.js",
      "import": "./dist/sweetalert2-preset.mjs",
      "browser": "./dist/sweetalert2-preset.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/sweetalert2-preset.umd.js",
  "module": "./dist/sweetalert2-preset.mjs",
  "unpkg": "./dist/sweetalert2-preset.umd.js",
  "jsdelivr": "./dist/sweetalert2-preset.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "vitest run && pnpm build && cl release"
  },
  "peerDependencies": {
    "sweetalert2": ">=11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@vitest/coverage-c8": "latest",
    "@vitest/ui": "latest",
    "case-police": "latest",
    "eslint": "latest",
    "happy-dom": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "sweetalert2": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "lint-staged": {
    "!(demo/**/*)*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}