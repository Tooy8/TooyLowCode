log ref='jump'></JumpDialog>\r\n\r\n  </config-item>\r\n</template>\r\n\r\n<script>\r\nimport schemaMixin from \"@/mixin/schemaMixin\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"SchemaJump\",\r\n\r\n  mixins: [schemaMixin],\r\n\r\n  provide() {\r\n    return {\r\n      value: this.value,\r\n    };\r\n  },\r\n\r\n  props: {\r\n    value: {\r\n      default: () => ({}),\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"project\", \"fixedPages\"]),\r\n    pageName() {\r\n      let page = null;\r\n      switch (this.mValue.type) {\r\n        case \"fixed\":\r\n          page = this.fixedPages.find((page) => page.id == this.mValue.id);\r\n          break;\r\n        case \"custom\":\r\n          page = this.project.pages.find((page) => page.id == this.mValue.id);\r\n          break;\r\n        case \"link\":\r\n          page = { name: \"外链h5\" };\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return page.name;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    open() {\r\n      this.$refs.jump.open();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}