/plugins/InsertImage","sourcesContent":["<template>\r\n  <el-dialog\r\n    :visible=\"show\"\r\n    title=\"插入图片\"\r\n    :append-to-body=\"true\"\r\n    :close-on-click-modal=\"false\"\r\n    destroy-on-close\r\n    @close=\"show=false\"\r\n  >\r\n    <el-form ref=\"formRef\" :model=\"formData\">\r\n      <el-form-item\r\n        prop=\"src\"\r\n        :rules=\"{required:true,message:'必填项'}\"\r\n      >\r\n        <Imgpond v-model=\"formData.src\" valueType=\"array\"/>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <el-button @click=\"show=false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"confirm\">确 定</el-button>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport 'imgpond/dist/style.css'\r\nimport Imgpond from 'imgpond'\r\n\r\nexport default {\r\n  components: { Imgpond },\r\n  props: {\r\n    editor: {\r\n      required: true,\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      show: false,\r\n      formData: {\r\n        src: []\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    show: {\r\n      handler (newVal, oldVal) {\r\n        if (!newVal) {\r\n          this.formData = {\r\n            src: []\r\n          }\r\n        }\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    open () {\r\n      this.show = true\r\n    },\r\n    confirm () {\r\n      this.$refs.formRef.validate(valid => {\r\n        if (valid) {\r\n          this.formData.src.map(v => {\r\n            this.editor.insertContent(`<img src=${v}>`)\r\n          })\r\n          this.show = false\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .el-dialog {\r\n  min-width: 600px;\r\n}\r\n</style>\r\n"]}]}