te v-else>\r\n        <el-tag size='small'>{{pageName}}</el-tag>\r\n        <span class=\"ml5 f12 f-theme pointer\" @click=\"open\">修改</span>\r\n      </template>\r\n    </template>\r\n\r\n    <JumpDialog ref='jump'></JumpDialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapMutations, mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"JumpSelect\",\r\n  provide() {\r\n    return {\r\n      value: this.value,\r\n    };\r\n  },\r\n  props: {\r\n    value: {\r\n      default: () => ({}),\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      mValue: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"project\",\"fixedPages\"]),\r\n    pageName() {\r\n      let page = null;\r\n      switch (this.mValue.type) {\r\n        case \"fixed\":\r\n          page = this.fixedPages.find(\r\n            (page) => page.id == this.mValue.id\r\n          );\r\n          break;\r\n        case \"custom\":\r\n          page = this.project.pages.find((page) => page.id == this.mValue.id);\r\n          break;\r\n        case \"link\":\r\n          page = { name: \"外链h5\" };\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return page.name;\r\n    },\r\n  },\r\n  watch: {\r\n    value: {\r\n      immediate: true,\r\n      handler(newValue, oldValue) {\r\n        this.mValue = newValue;\r\n        console.log('mValue');\r\n        console.log(this.mValue);\r\n        \r\n        \r\n      },\r\n    },\r\n    mValue: {\r\n      immediate: true,\r\n      handler(newValue, oldValue) {\r\n        this.$emit(\"input\", newValue);\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    open() {\r\n      this.$refs.jump.open();\r\n    },\r\n\r\n    getName() {\r\n      console.log(\"触发getName\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}