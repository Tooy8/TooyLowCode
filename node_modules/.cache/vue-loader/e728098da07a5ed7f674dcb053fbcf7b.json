ass=\"[item.jump.id == activeTab ? 'f-h5-theme' : '']\"\r\n      @click=\"changeTab(item)\"\r\n    >\r\n      <i class=\"icon f22\" :class=\"item.icon\"></i>\r\n      <span class=\"f13\">{{ item.text }}</span>\r\n    </li>\r\n  </ul>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Navigation',\r\n  props: {\r\n    styles: {\r\n      type: Object,\r\n      default: () => {}\r\n    },\r\n    list: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    active: {\r\n      type: String\r\n    },\r\n    disabled: {\r\n      type: Boolean\r\n    }\r\n  },\r\n\r\n  data () {\r\n    return {\r\n      activeTab: ''\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    active: {\r\n      immediate: true,\r\n      handler () {\r\n        this.activeTab = this.active\r\n      }\r\n    },\r\n\r\n    activeTab: {\r\n      handler () {\r\n        this.$emit('update:active', this.activeTab)\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    // tabs 动态样式\r\n    getTabsStyle () {\r\n      return {\r\n        // backgroundColor: this.style?.background || '#fff'\r\n        backgroundColor: '#fff'\r\n      }\r\n    },\r\n\r\n    // 切换tab\r\n    changeTab (item) {\r\n      if (this.activeTab == item.jump.id) return\r\n\r\n      this.activeTab = item.jump.id\r\n\r\n      // 设置模式置灰，禁止点击跳转\r\n      if (this.disabled) return\r\n\r\n      this.$jump(item.jump)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.tabs {\r\n  display: flex;\r\n  width: 100%;\r\n  height: 56px;\r\n  border-top: solid 1px #9b9b9b20;\r\n\r\n  .tabs-item {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex: 1;\r\n    background-size: contain;\r\n    font-size: 10px;\r\n    color: #9b9b9b;\r\n\r\n    i {\r\n      font-size: 20px;\r\n      margin-bottom: 3px;\r\n    }\r\n  }\r\n\r\n  .tabs-item-select {\r\n    color: #f20c25;\r\n  }\r\n}\r\n</style>\r\n"]}]}