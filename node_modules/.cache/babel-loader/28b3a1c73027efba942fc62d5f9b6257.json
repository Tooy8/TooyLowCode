tTime: 2022-03-17 14:23:25\r\n-->\r\n<template>\r\n  <div>\r\n    <!-- 搜索框 -->\r\n    <!-- <van-search\r\n      v-model=\"searchVal\"\r\n      show-action\r\n      shape=\"round\"\r\n      placeholder=\"搜索商品\"\r\n    >\r\n      <template #action>\r\n        <div class=\"pl5 pr5 f-h5-theme\" @click=\"onSearch\">搜索</div>\r\n      </template>\r\n    </van-search> -->\r\n\r\n    <div class=\"sercarh\">\r\n      <div class=\"sercarh-body\">\r\n        <i class=\"el-icon-search mr5\"></i>\r\n        请输入关键词\r\n      </div>\r\n      <div class=\"sercarh-btn\">搜索</div>\r\n    </div>\r\n\r\n    <!-- 热门搜索 -->\r\n    <div class=\"pl15 pr15 mt10 f14\">\r\n      <div>热门搜索</div>\r\n      <ul class=\"flex flex-wrap\">\r\n        <li\r\n          class=\"hot flex-center\"\r\n          v-for=\"(item, index) in project.config.hotList\"\r\n          :key=\"index\"\r\n          @click=\"onHot(item)\"\r\n        >\r\n          <img src=\"https://b.yzcdn.cn/search-page/hot-fire.png\" alt=\"\" />\r\n          <span>\r\n            {{ item.value }}\r\n          </span>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"SearchBar\",\r\n\r\n  data() {\r\n    return {\r\n      searchVal: \"\",\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"project\"]),\r\n  },\r\n\r\n  methods: {\r\n    onHot(item) {\r\n      this.$router.push({ name: \"goods-list\", query: { search: item.value } });\r\n    },\r\n\r\n    onSearch() {\r\n      this.$router.push({\r\n        name: \"goods-list\",\r\n        query: { search: this.searchVal },\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.hot {\r\n  margin: 8px 8px 0px 0;\r\n  padding: 6px 16px;\r\n  border-radius: 20px;\r\n  font-size: 13px;\r\n  color: #f44;\r\n  border-color: rgba(254, 68, 68, 0.1);\r\n  border: 1px solid #ebedf0;\r\n\r\n  img {\r\n    width: 16px;\r\n    height: 16px;\r\n    margin-right: 5px;\r\n  }\r\n}\r\n\r\n.sercarh {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px;\r\n\r\n  .sercarh-body {\r\n    display: flex;\r\n    align-items: center;\r\n    width: 330px;\r\n    height: 30px;\r\n    margin: 0 6px 0 12px;\r\n    padding-left: 9px;\r\n    background: #f7f8fa;\r\n    border-radius: 15px;\r\n    color: #d6d6d9;\r\n    font-size: 13px;\r\n  }\r\n\r\n  .sercarh-btn {\r\n    width: 48px;\r\n    height: 26px;\r\n    color: #fff;\r\n    border-radius: 13px;\r\n    line-height: 26px;\r\n    font-size: 12px;\r\n    text-align: center;\r\n    background: linear-gradient(90deg, #ef4c2d, #d40908);\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components/SearchBar"}]}