CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,mBAAA,IAAA,CAAA,EAAA,IAAA,KAAA,CAAA,MAAA,CAAA,EAAA;AAAA,WAAA,CAAA;AACA;;AACA,aAAA,MAAA;AACA,UAAA,IAAA,GAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA;;AACA;AACA;AAXA;;AAaA,aAAA,IAAA,CAAA,IAAA;AACA;AAlBA,IAjBA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA;AAHA;AAtCA,CAAA","sourcesContent":["<!--\r\n * @Description: 页面跳转选择组件\r\n * @Autor: WangYuan\r\n * @Date: 2021-08-12 11:06:37\r\n * @LastEditors: WangYuan\r\n * @LastEditTime: 2021-09-26 19:38:10\r\n-->\r\n\r\n<template>\r\n  <config-item :label='label'>\r\n    <div class=\"flex col-center h32\">\r\n      <div\r\n        v-if=\"!mValue.id\"\r\n        class=\"f12 f-theme pointer\"\r\n        @click=\"open\"\r\n      >\r\n        选择跳转到的页面\r\n      </div>\r\n\r\n      <template v-else>\r\n        <el-tag size='small'>{{pageName}}</el-tag>\r\n        <span\r\n          class=\"ml5 f12 f-theme pointer\"\r\n          @click=\"open\"\r\n        >修改</span>\r\n      </template>\r\n    </div>\r\n\r\n    <JumpDialog ref='jump'></JumpDialog>\r\n\r\n  </config-item>\r\n</template>\r\n\r\n<script>\r\nimport schemaMixin from \"@/mixin/schemaMixin\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"SchemaJump\",\r\n\r\n  mixins: [schemaMixin],\r\n\r\n  provide() {\r\n    return {\r\n      value: this.value,\r\n    };\r\n  },\r\n\r\n  props: {\r\n    value: {\r\n      default: () => ({}),\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"project\", \"fixedPages\"]),\r\n    pageName() {\r\n      let page = null;\r\n      switch (this.mValue.type) {\r\n        case \"fixed\":\r\n          page = this.fixedPages.find((page) => page.id == this.mValue.id);\r\n          break;\r\n        case \"custom\":\r\n          page = this.project.pages.find((page) => page.id == this.mValue.id);\r\n          break;\r\n        case \"link\":\r\n          page = { name: \"外链h5\" };\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return page.name;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    open() {\r\n      this.$refs.jump.open();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/custom-schema-template/components/SchemaJump"}]}